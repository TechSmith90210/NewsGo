{
  "expo": {
    "name": "NewsGo",
    "slug": "NewsGo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appLogo/newspaper_light.png",
    "scheme": "newsgo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appLogo/newspaper_light.png",
        "backgroundColor": "#1b1b1b",
        "resizeMode": "contain"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.shksalman_dev.NewsGo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/appLogo/newspaper_light.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1b1b1b"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}